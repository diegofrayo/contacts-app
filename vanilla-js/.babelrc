{
	"presets": ["@babel/preset-env"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./src"],
				"alias": {
					"~/components": "./src/components",
					"~/config": "./src/config",
					"~/lib": "./src/lib",
					"~/modules": "./src/modules",
					"~/types": "./src/types",
					"~/utils": "./src/utils"
				}
			}
		]
	]
}
